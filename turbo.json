{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "stylelint:fix": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "build-storybook": {
      "outputs": ["dist/**"]
    },
    "storybook": {
      "outputs": ["storybook-static/**"],
      "dependsOn": ["build-storybook"],
      "cache": false
    }
  },
  "remoteCache": {
    "signature": true
  }
}
