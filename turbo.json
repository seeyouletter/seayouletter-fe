{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "lint": {
      "outputs": [],
      "inputs": []
    },
    "stylelint": {
      "outputs": [],
      "inputs": []
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "storybook-static/**"]
    },
    "dev": {
      "cache": false
    },
    "build-storybook": {
      "outputs": ["storybook-static/**"]
    },
    "storybook": {
      "dependsOn": ["build-storybook"],
      "cache": false
    }
  },
  "remoteCache": {
    "signature": true
  }
}
